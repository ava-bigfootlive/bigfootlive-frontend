{
  "name": "frontend-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "type-check": "tsc --noEmit",
    "css:analyze": "node scripts/detect-css-conflicts.js",
    "css:lint": "stylelint src/**/*.css",
    "css:lint:fix": "stylelint src/**/*.css --fix",
    "css:purge": "purgecss --config purgecss.config.js --output temp-purged/",
    "css:utils": "node scripts/create-utilities.js",
    "css:clean": "npm run css:analyze && npm run css:utils && npm run css:lint:fix",
    "ui:migrate": "node scripts/migrate-ui-system.js src/ --fix-a11y",
    "ui:migrate:dry": "node scripts/migrate-ui-system.js --dry-run --verbose",
    "ui:migrate:components": "node scripts/codemods/migrate-ui-components.js src/",
    "ui:migrate:tokens": "node scripts/codemods/integrate-theme-tokens.js src/",
    "ui:audit:a11y": "node scripts/codemods/audit-accessibility.js src/ --verbose",
    "ui:fix:a11y": "node scripts/codemods/audit-accessibility.js src/ --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "qa:lighthouse": "playwright test e2e/tests/lighthouse-audit.spec.ts",
    "qa:cross-browser": "playwright test e2e/tests/cross-browser-quality.spec.ts",
    "qa:all": "npm run qa:cross-browser && npm run qa:lighthouse",
    "qa:accessibility": "playwright test e2e/tests/cross-browser-quality.spec.ts --grep 'Accessibility'",
    "build:production": "NODE_ENV=production vite build",
    "qa:suite": "node scripts/run-qa-suite.js",
    "qa:suite:fast": "node scripts/run-qa-suite.js --skip-lighthouse --browsers=chromium,firefox",
    "test:auth": "TEST_EMAIL=apvantaio@gmail.com TEST_PASSWORD=DisneyDemo2025# npx playwright test",
    "test:auth:setup": "TEST_EMAIL=apvantaio@gmail.com TEST_PASSWORD=DisneyDemo2025# npx playwright test --project=setup",
    "test:auth:complete": "TEST_EMAIL=apvantaio@gmail.com TEST_PASSWORD=DisneyDemo2025# npx playwright test e2e/complete-platform.spec.ts",
    "test:auth:features": "TEST_EMAIL=apvantaio@gmail.com TEST_PASSWORD=DisneyDemo2025# npx playwright test e2e/authenticated-features.spec.ts"
  },
