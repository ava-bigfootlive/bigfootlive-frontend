{
  "name": "bigfootlive-backend",
  "version": "1.0.0",
  "description": "Live streaming platform backend with enterprise authentication and SSO",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:ts": "nodemon src/server.ts",
    "build": "tsc",
    "build:start": "tsc && node dist/server.js",
    "container:dev": "nodemon src/event-container/container.ts",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "lint": "eslint src/**/*.{js,ts}",
    "lint:fix": "eslint src/**/*.{js,ts} --fix",
    "format": "prettier --write src/**/*.{js,ts}",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js"
  },
  "dependencies": {
    "aws-sdk": "^2.1509.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.2.1",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-media-server": "^2.6.0",
    "nodemailer": "^6.9.7",
    "passport": "^0.6.0",
    "passport-oauth2": "^1.7.0",
    "passport-openidconnect": "^0.1.1",
    "passport-saml": "^3.2.4",
    "qrcode": "^1.5.3",
    "rate-limiter-flexible": "^4.0.1",
    "redis": "^4.6.11",
    "saml2-js": "^4.0.4",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "speakeasy": "^2.0.0",
    "twilio": "^4.15.0",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "socket.io-client": "^4.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "streaming",
    "live",
    "rtmp",
    "websocket",
    "analytics",
    "chat",
    "authentication",
    "sso",
    "saml",
    "oidc",
    "enterprise",
    "security"
  ],
  "author": "BigFoot Live Team",
  "license": "MIT"
}
